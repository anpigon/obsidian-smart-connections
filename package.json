{
  "name": "obsidian-smart-connections",
  "version": "2.0.117",
  "description": "Link and chat with your notes using ChatGPT and embedding artificial intelligence from OpenAI.",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "compile": "node build/compile_ejs.js",
    "release": "node build/release.js",
    "test": "npx ava --verbose test",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianpetro/obsidian-smart-connections.git"
  },
  "keywords": [
    "Obsidian",
    "Smart",
    "Connections",
    "AI",
    "ChatGPT",
    "Embeddings",
    "Notes"
  ],
  "author": "WFH Brian (Brian Petro)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/brianpetro/obsidian-smart-connections/issues"
  },
  "homepage": "https://smartconnections.app",
  "devDependencies": {
    "@xenova/transformers": "^2.15.0",
    "archiver": "^6.0.1",
    "ava": "^6.1.1",
    "axios": "^1.6.7",
    "child_process": "^1.0.2",
    "dotenv": "^16.3.1",
    "esbuild": "^0.17.19",
    "obsidian": "latest",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "ejs": "^3.1.9",
    "js-tiktoken": "^1.0.10",
    "smart-chunks": "latest",
    "smart-collections": "latest",
    "smart-embed": "^1.0.6"
  }
}
